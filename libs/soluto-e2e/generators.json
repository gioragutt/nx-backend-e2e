{
  "$schema": "http://json-schema.org/schema",
  "name": "soluto-e2e",
  "version": "0.0.1",
  "generators": {
    "e2e-project": {
      "factory": "./src/generators/e2e-project/generator",
      "schema": "./src/generators/e2e-project/schema.json",
      "description": "Generate an E2E project for a a backend app"
    },
    "wiremock": {
      "factory": "./src/generators/wiremock/generator",
      "schema": "./src/generators/wiremock/schema.json",
      "description": "Generate wiremock boilerplate to the backend e2e project"
    }
  }
}
